<html lang="en">
<head>

    <meta charset="utf-8" />
    <title>Overview - KITE Dashboard</title>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/pace/1.0.0/pace.min.js"></script>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/pace/1.0.0/themes/black/pace-theme-minimal.css" rel="stylesheet" />
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" rel="stylesheet" />
    <link href="assets/css/light-bootstrap-dashboard.css" rel="stylesheet"/>
    <link href="assets/css/kite-dashboard.css" rel="stylesheet"/>

    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <link href="https://rawgithub.com/olimsaidov/pixeden-stroke-7-icon/master/pe-icon-7-stroke/dist/pe-icon-7-stroke.css" rel="stylesheet" />

    <script type="text/javascript" src="assets/js/jquery-git.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.1.4/Chart.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/1.10.2/jquery.js" type="text/javascript"></script>
    <script src="assets/js/2D.js"></script>

</head>
<body>
    <div class="wrapper">
     <!-- Start of side nav bar -->
        <div class="sidebar" data-color="black" data-image="assets/img/sidebarSmall.jpg">
            <div class="sidebar-wrapper">
                <div class="logo">
                    <a href="dashboard" class="simple-text">
                        KITE
                    </a>
                </div>

                <ul class="nav">
                    <li>
                        <div class="row">
                            <div class="col-sm-1 col-sm-offset-1">
                                <i class="pe-7s-search"></i>
                            </div>
                            <div class="col-sm-9 ">
                                <form action="search" method="get">
                                  <div>
                                    <input name="key" id="key" placeholder="Search">
                                  </div>
                                </form>
                            </div>

                        </div>
                    </li>
                    <li>
                        <a href="dashboard"> <!-- change link to dashboard -->
                            <i class="pe-7s-note2"></i>
                            <p>Dashboard</p>
                        </a>
                    </li>
                    <li>
                        <a href="score">
                            <i class="pe-7s-keypad"></i>
                            <p>Scoreboard</p>
                        </a>
                    </li>
                    <li>
                        <a href="versions">
                            <i class="pe-7s-way"></i>
                            <p>Version Info</p>
                        </a>
                    </li>
                    <li class="active">
                        <nav class="overview">
                            <ul>
                                <li><a href="#">
                                        <i class="pe-7s-pin"></i>
                                        <p>Overview</p>
                                    </a>
                                    <ul class="result-wrapping">
                                        #foreach($test in $listOfTest)
                                        #if($test.getTupleSize()==2)
                                          <li><a href="overview?testname=$linkTool.encode($test.getTestName())&1v1=true" style="font-size: 12px;" >$test.getTestName()<div class="circle"></div></a></li>
                                        #else
                                          <li><a href="overview?testname=$linkTool.encode($test.getTestName())&1v1=false" style="font-size: 12px;" >$test.getTestName()<div class="circle"></div></a></li>
                                        #end
                                        #end
                                    </ul>
                                </li>
                            </ul>
                        </nav>
                    </li>
                </ul>
            </div>
        </div>
        <!-- End of side nav bar -->
        <!-- Start main data content display -->
        <div class="main-panel">
        <!-- Start horizontal nav bar -->
            <nav class="navbar navbar-default navbar-fixed">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#">
                            <i class="pe-7s-menu"></i>
                        </button>
                        <a class="navbar-brand" href="dashboard">Dashboard</a>
                    </div>
                    <div class="collapse navbar-collapse">
                    </div>
                </div>
            </nav>
        <!-- End of horizontal nav bar -->
            <div class="content">
                <div class="container-fluid">
                      <div class="card">
                          <div class="content">
                              <h3 >$testname</h3>
                              <h4 class="medium-boy">$description</h4>
                              <h5 class="small-boy">(Results for test cases between the latest stable version of browsers)</h5>
                              <div class="dropdown">
                                  <button type="button" class="btn pick-option dropdown-toggle dropdown-button" data-toggle="dropdown"><i class="pe-7s-angle-down-circle big-boy"></i>$dateTool.format($startTime)</button>
                                  <ul class="dropdown-menu">
                                      #set($index1 = 1)
                                      #foreach($time in $listOfRuns)
                                        <li><a href ="overview?testname=$linkTool.encode($testname)&1v1=true&time=$linkTool.encode($time)">$index1. $dateTool.format($time)</a></li>
                                      #set($index1 = $index1 + 1)
                                      #end
                                  </ul>
                              </div>
                              <table class="table table-hover square" id="resultMatrix" style="table-layout:fixed">
                              </table>
                          </div>
                      </div>
                      <script>
                          myData=$jsonData;
                          displayMatrix(myData.results, $('#resultMatrix'));
                      </script>
                  </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End of main data content display -->
</body>
    <!--   Core JS Files   -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="assets/js/light-bootstrap-dashboard.js"></script>
</html>