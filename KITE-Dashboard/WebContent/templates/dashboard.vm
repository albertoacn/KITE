<html lang="en">
<head>

    <meta charset="utf-8" />
    <title>KITE Test Dashboard</title>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/pace/1.0.0/pace.min.js"></script>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/pace/1.0.0/themes/black/pace-theme-minimal.css" rel="stylesheet" />
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" rel="stylesheet" />
    <link href="assets/css/light-bootstrap-dashboard.css" rel="stylesheet"/>
    <link href="assets/css/kite-dashboard.css" rel="stylesheet"/>

    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <link href="https://rawgithub.com/olimsaidov/pixeden-stroke-7-icon/master/pe-icon-7-stroke/dist/pe-icon-7-stroke.css" rel="stylesheet" />

    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.1.4/Chart.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/1.10.2/jquery.js" type="text/javascript"></script>
    <script src="assets/js/dashboard.js"></script>
    <script>
        var stats = [[],[],[]];
        var config_labels = [];
        var labels = [];
        var graphMap = new Map();
    </script>

</head>
<body>
    <div class="wrapper">
        <!-- Start of side nav bar -->
        <div class="sidebar" data-color="black" data-image="assets/img/sidebarSmall.jpg">
            <div class="sidebar-wrapper">
                <div class="logo">
                    <a href="dashboard" class="simple-text">
                        KITE
                    </a>
                </div>
                <ul class="nav">
                    <li>
                        <div class="row">
                            <div class="col-sm-1 col-sm-offset-1">
                                <i class="pe-7s-search"></i>
                            </div>
                            <div class="col-sm-9 ">
                                <form action="search" method="get">
                                  <div>
                                    <input name="key" id="key" placeholder="Search">
                                  </div>
                                </form>
                            </div>

                        </div>
                    </li>
                    <li class="active">
                        <a href="dashboard">
                            <i class="pe-7s-note2"></i>
                            <p>Dashboard</p>
                        </a>
                    </li>
                    <li>
                        <a href="score">
                            <i class="pe-7s-keypad"></i>
                            <p>Scoreboard</p>
                        </a>
                    </li>
                    <li>
                        <a href="versions">
                            <i class="pe-7s-way"></i>
                            <p>Version Info</p>
                        </a>
                    </li>
                    <li>
                        <nav class="overview">
                            <ul>
                                <li><a href="#">
                                        <i class="pe-7s-pin"></i>
                                        <p>Overview</p>
                                    </a>
                                    <ul class="result-wrapping">
                                        #foreach($test in $listOfTest)
                                        #if($test.getTupleSize()==2)
                                          <li><a href="overview?testname=$linkTool.encode($test.getTestName())&1v1=true" style="font-size: 12px;" >$test.getTestName()<div class="circle"></div></a></li>
                                        #else
                                          <li><a href="overview?testname=$linkTool.encode($test.getTestName())&1v1=false" style="font-size: 12px;" >$test.getTestName()<div class="circle"></div></a></li>
                                        #end
                                        #end
                                    </ul>
                                </li>
                            </ul>
                        </nav>
                    </li>
                </ul>
            </div>
        </div>
        <!-- End of side nav bar -->
        <!-- Start main data content display -->
        <div class="main-panel">
        <!-- Start horizontal nav bar -->
            <nav class="navbar navbar-default navbar-fixed">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#">
                            <i class="pe-7s-menu"></i>
                        </button>
                        <a class="navbar-brand" href="dashboard">Dashboard</a>
                    </div>
                    <div class="collapse navbar-collapse">
                    </div>
                </div>
            </nav>
        <!-- End of horizontal nav bar -->
            <div class="content">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-4 col-md-12 col-sm-12">
                            <div class="card">
                                <div class="content">
                                    <h4 class="title">Configurations Executed: </h4>
                                </div>
                            </div>
                            <div class="config-wrapping">
                                #if($listOfConfig.size()>0)
                                #set($index = $listOfConfig.size())
                                #foreach($config in $listOfConfig)
                                    <div id="config$index" class="grow card">
                                        <div><i name="config$index" class="pe-7s-angle-down-circle green"></i>
                                        <a class="config-name" href ="execution?name=$linkTool.encode($config.getConfigName())">$index. $config.getConfigName() - $config.getNumberOfRuns() run(s)</a></div>
                                        <a class="last-run" href ="tests?id=$linkTool.encode($config.getLatestId())&configname=$linkTool.encode($config.getConfigName())">(Last run: $dateTool.format($config.getLatestRun()))</a>
                                        <table class="table">
                                                <tr>
                                                    <td width:"10%">
                                                        <div class="dropdown">
                                                            <button type="button" class="btn show-more dropdown-toggle" data-toggle="dropdown"><i class="pe-7s-photo-gallery"></i></button>
                                                            <ul class="dropdown-menu">
                                                               <li><a href ="execution?name=$linkTool.encode($config.getConfigName())">See all</a></li>
                                                               #set($index1 = 1)
                                                               #foreach($startTime in $config.getStartTimeList())
                                                               <li><a href ="tests?id=$linkTool.encode($config.getId($startTime))&configname=$linkTool.encode($config.getConfigName())">$index1. $dateTool.format($startTime)</a></li>
                                                               #set($index1 = $index1 + 1)
                                                               #end
                                                            </ul>
                                                        </div>
                                                    </td>
                                                    <td width:"10%">
                                                        <button type="button" class="btn show-more"><i class="pe-7s-graph2"></i></button>
                                                    </td>
                                                    <td width:"80%">
                                                        <i class="pe-7s-check green" data-toggle="tooltip" data-placement="top" title="Success"></i><a>$config.getNumberOfSuccess()</a>
                                                        <i class="pe-7s-close-circle red" data-toggle="tooltip" data-placement="top" title="Failed"></i><a>$config.getNumberOfFailed()</a>
                                                        <i class="pe-7s-attention blue" data-toggle="tooltip" data-placement="top" title="Error"></i><a>$config.getNumberOfError()</a>
                                                        <i class="pe-7s-timer grey" data-toggle="tooltip" data-placement="top" title="Pending"></i><a>$config.getNumberOfPending()</a>
                                                        <a>Total:$config.getNumberOfTested()</a>
                                                    </td>
                                                </tr>
                                        </table>
                                        <div class="chart-container" style="position: relative; height:150px; width:230px">
                                            <canvas id="$config.getConfigName()"></canvas>
                                            <script>
                                                var success = $config.getNumberOfSuccess();
                                                var failed = $config.getNumberOfFailed();
                                                var error = $config.getNumberOfError();
                                                var pending = $config.getNumberOfPending();
                                                var lastrun = '$dateTool.format('dd-MMM-yyyy',$config.getLatestRun())';
                                                config_labels.unshift(lastrun);
                                                stats[0].unshift(success);
                                                stats[1].unshift(failed);
                                                stats[2].unshift(error);
                                                var ctx = document.getElementById("$config.getConfigName()").getContext('2d');
                                                var myChart = new Chart(ctx, {
                                                type: 'pie',
                                                data: {
                                                    labels: ["Success", "Failed", "Error", "Pending"],
                                                    datasets: [{
                                                        backgroundColor: [
                                                        "#42f4aa",
                                                        "#ff4b30",
                                                        "#30b3ff",
                                                        "grey"
                                                        ],
                                                        data: [success, failed, error, pending]
                                                        }]
                                                    },
                                                options: {
                                                    responsive: false,
                                                    legend: {
                                                        display: false,
                                                        position: 'top',
                                                        labels: {
                                                            boxWidth: 30,
                                                            fontSize: 12
                                                            }
                                                        }
                                                    }
                                                });
                                            </script>
                                        </div>
                                    </div>
                                    #set($index = $index - 1)
                                #end
                                #end
                            </div>
                        </div>
                        <div class="col-lg-8 col-md-12 col-sm-12">
                            <div class="row">
                                <div class="col-lg-8 col-md-12 col-sm-12">
                                    <div class="card">
                                        <div class="header">
                                            <h4 class="title">Results throughout configurations:</h4>
                                            </br>
                                        </div>
                                        <div class="content">
                                            <canvas id="overtime-result"></canvas>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-4 col-md-12 col-sm-12">
                                    <div class="card">
                                        <div class="header">
                                            <h4 class="title">Browsers tested with:</h4>
                                            </br>
                                        </div>
                                        <div class="content search-wrapping">
                                            #foreach($browser in $listOfBrowser)
                                                <h5 class="small-boy">
                                                #if ($browser.getName() == "firefox")
                                                    <img src="assets/img/firefox.png" height="30" width="30">
                                                #elseif ($browser.getName() == "chrome")
                                                    <img src="assets/img/chrome.png" height="30 width="30">
                                                #elseif ($browser.getName() == "MicrosoftEdge")
                                                    <img src="assets/img/edge.png" height="30" width="30">
                                                #elseif ($browser.getName() == "safari")
                                                    <img src="assets/img/safari.png" height="30" width="30">
                                                #end

                                                $browser.getVersion()
                                                $browser.getPlatform()</h5>
                                            #end
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="card">
                                <div class="content stat-container">
                                    <h4 class="title">Stats across runs: </h4>
                                    <div class="row">
                                        <div class="col-lg-4 col-md-12 col-sm-12">
                                            <div id="chosen-test" class="styled-select-test">
                                            <i id="submit" class="pe-7s-search"></i>
                                                <select>
                                                    <option>Choose a test</option>
                                                    #foreach($test in $listOfTest)
                                                    <option>$test.getTestName()</option>
                                                    #end
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-lg-4 col-md-12 col-sm-12">
                                            <div id="chosen-caller" class="styled-select-caller">
                                                <select>
                                                    <option>Caller</option>
                                                    #foreach($browser in $listOfBrowser)
                                                        #if ($browser.getName()=="MicrosoftEdge")
                                                            #if ($browser.getVersion().length()<=9)
                                                            <option>
                                                                $browser.getName()_$browser.getVersion()_$browser.getPlatform()
                                                            </option>
                                                            #end
                                                        #else)
                                                            #if ($browser.getVersion().length()<=5)
                                                            <option>
                                                                $browser.getName()_$browser.getVersion()_$browser.getPlatform()
                                                            </option>
                                                            #end
                                                        #end
                                                    #end
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-lg-4 col-md-12 col-sm-12">
                                            <div id="chosen-callee" class="styled-select-callee">
                                                <select>
                                                    <option>Callee</option>
                                                    #foreach($browser in $listOfBrowser)
                                                        #if ($browser.getName()=="MicrosoftEdge")
                                                            #if ($browser.getVersion().length()<=9)
                                                            <option>
                                                                $browser.getName()_$browser.getVersion()_$browser.getPlatform()
                                                            </option>
                                                            #end
                                                        #else)
                                                            #if ($browser.getVersion().length()<=5)
                                                            <option>
                                                                $browser.getName()_$browser.getVersion()_$browser.getPlatform()
                                                            </option>
                                                            #end
                                                        #end
                                                    #end
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="stat-content">
                                        <h5 class="medium-boy" id="stat-status"></h5>
                                        <ul class="nav nav-tabs">
                                            <li class="active"><a class="medium-boy" data-toggle="tab" href="#video" style="cursor:pointer">Video</a></li>
                                            <li><a class="medium-boy" data-toggle="tab" href="#audio" style="cursor:pointer">Audio</a></li>
                                        </ul>

                                    <div class="tab-content">
                                        <div id="video" class="tab-pane fade in active">
                                            <div class="row">
                                                <div class="col-lg-5 col-md-12 col-sm-12">
                                                    <canvas id="overtime-video-bytes-sent"></canvas>
                                                </div>
                                                <div class="col-lg-5 col-md-12 col-sm-12">
                                                    <canvas id="overtime-video-bytes-received"></canvas>
                                                </div>
                                                <div class="col-lg-5 col-md-12 col-sm-12">
                                                    <canvas id="overtime-video-packets-sent"></canvas>
                                                </div>
                                                <div class="col-lg-5 col-md-12 col-sm-12">
                                                    <canvas id="overtime-video-packets-received"></canvas>
                                                </div>
                                            </div>
                                        </div>
                                        <div id="audio" class="tab-pane fade">
                                            <div class="row">
                                                <div class="col-lg-5 col-md-12 col-sm-12">
                                                    <canvas id="overtime-audio-bytes-sent"></canvas>
                                                </div>
                                                <div class="col-lg-5 col-md-12 col-sm-12">
                                                    <canvas id="overtime-audio-bytes-received"></canvas>
                                                </div>
                                                <div class="col-lg-5 col-md-12 col-sm-12">
                                                    <canvas id="overtime-audio-packets-sent"></canvas>
                                                </div>
                                                <div class="col-lg-5 col-md-12 col-sm-12">
                                                    <canvas id="overtime-audio-packets-received"></canvas>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        <!-- End of main data content display -->
        </div>
    </div>
</body>
    <!--   Core JS Files   -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="assets/js/light-bootstrap-dashboard.js"></script>
</html>